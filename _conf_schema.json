{
  "gateway_ws_url": {
    "description": "QQ 农场网关 WebSocket 地址",
    "type": "string",
    "default": "wss://gate-obt.nqf.qq.com/prod/ws"
  },
  "client_version": {
    "description": "客户端版本号（用于登录握手）",
    "type": "string",
    "default": "1.6.0.5_20251224"
  },
  "platform": {
    "description": "平台标识，当前仅支持 qq",
    "type": "string",
    "default": "qq"
  },
  "heartbeat_interval_sec": {
    "description": "心跳间隔（秒）",
    "type": "int",
    "default": 25
  },
  "rpc_timeout_sec": {
    "description": "单次 RPC 超时（秒）",
    "type": "int",
    "default": 10
  },
  "request_timeout_sec": {
    "description": "命令请求超时（秒）",
    "type": "int",
    "default": 15
  },
  "allowed_user_ids": {
    "description": "允许使用命令的用户白名单（静态）",
    "type": "list",
    "default": []
  },
  "allowed_group_ids": {
    "description": "允许使用命令的群白名单（静态）",
    "type": "list",
    "default": []
  },
  "rate_limit_read_sec": {
    "description": "同用户读命令冷却秒数",
    "type": "float",
    "default": 1.0
  },
  "rate_limit_write_sec": {
    "description": "同用户写命令冷却秒数",
    "type": "float",
    "default": 2.0
  },
  "global_concurrency": {
    "description": "全局并发上限",
    "type": "int",
    "default": 20
  },
  "account_write_serialized": {
    "description": "同账号写操作是否串行化",
    "type": "bool",
    "default": true
  },
  "enable_image_render": {
    "description": "是否启用文本结果图片渲染",
    "type": "bool",
    "default": true
  },
  "render_service_url": {
    "description": "文转图服务地址",
    "type": "string",
    "default": "http://172.17.0.1:51234"
  },
  "render_timeout_sec": {
    "description": "图片渲染请求超时（秒）",
    "type": "int",
    "default": 30
  },
  "render_healthcheck_sec": {
    "description": "图片渲染健康检查超时（秒）",
    "type": "int",
    "default": 3
  }
}
